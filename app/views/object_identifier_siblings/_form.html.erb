<%= simple_form_for(@object_identifier_sibling) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :identifier %>
    <%= f.input :id_type %>
    <%= f.input :selector %>
    <%= f.association :object_identifier %>
    <%= f.association :sibling_relationship %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
