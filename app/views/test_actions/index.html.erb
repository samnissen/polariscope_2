<h1>Listing test_actions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Testset</th>
      <th>Activity</th>
      <th>Additional info</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_actions.each do |test_action| %>
      <tr>
        <td><%= test_action.name %></td>
        <td><%= test_action.description %></td>
        <td><%= test_action.testset %></td>
        <td><%= test_action.activity %></td>
        <td><%= test_action.additional_info %></td>
        <td><%= test_action.user %></td>
        <td><%= link_to 'Show', test_action %></td>
        <td><%= link_to 'Edit', edit_test_action_path(test_action) %></td>
        <td><%= link_to 'Destroy', test_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test action', new_test_action_path %>
