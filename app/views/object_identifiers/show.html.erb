<%- model_class = ObjectIdentifier -%>
<div class="page-header">
  <h1><%=t '.title', :default => ObjectIdentifier %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:identifier) %></dt>
      <dd><pre class="prettyprint"><%= @object_identifier.identifier %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:id_type) %></dt>
      <dd><pre class="prettyprint"><%= @object_identifier.id_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:selector) %></dt>
      <dd><pre class="prettyprint"><%= @object_identifier.selector %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              object_identifiers_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_object_identifier_path(@object_identifier), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              object_identifier_path(@object_identifier),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

