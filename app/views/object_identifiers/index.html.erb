<h1>Listing object_identifiers</h1>

<table>
  <thead>
    <tr>
      <th>Identifier</th>
      <th>Id type</th>
      <th>Selector</th>
      <th>Test action</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @object_identifiers.each do |object_identifier| %>
      <tr>
        <td><%= object_identifier.identifier %></td>
        <td><%= object_identifier.id_type %></td>
        <td><%= object_identifier.selector %></td>
        <td><%= object_identifier.test_action %></td>
        <td><%= object_identifier.user %></td>
        <td><%= link_to 'Show', object_identifier %></td>
        <td><%= link_to 'Edit', edit_object_identifier_path(object_identifier) %></td>
        <td><%= link_to 'Destroy', object_identifier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Object identifier', new_object_identifier_path %>
