<%- model_class = DataElementValue -%>
<div class="page-header">
  <h1>Edit Variable Value</h1>
</div>
<%= render :partial => 'form' %>

<%= form_for @data_element_value, :html => { :class => 'form-horizontal form-model' } do |f| %>
  <div class="form-group">
    <div class="col-md-offset-3 col-md-9">
      <%= link_to t('.destroy', :default => t('helpers.links.destroy')),
              data_element_value_path(@data_element_value),
              :method => :delete,
              :data => {
                :confirm => t('.confirm',
                :default => t("helpers.links.confirm", :default => 'Are you sure?'))
              },
              :class => 'btn btn-danger' %>
    </div>
  </div>
<% end %>
