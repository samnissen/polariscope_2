      <%= link_to t('.destroy', :default => t('helpers.links.destroy')),
              collection_testset_test_action_object_identifier_test_action_datum_path(@test_action_datum.object_identifier.test_action.testset.collection, @test_action_datum.object_identifier.test_action.testset, @test_action_datum.object_identifier.test_action, @test_action_datum.object_identifier, @test_action_datum),
