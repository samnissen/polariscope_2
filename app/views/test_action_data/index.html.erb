<h1>Listing test_action_data</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Test action</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_action_data.each do |test_action_datum| %>
      <tr>
        <td><%= test_action_datum.data %></td>
        <td><%= test_action_datum.test_action %></td>
        <td><%= link_to 'Show', test_action_datum %></td>
        <td><%= link_to 'Edit', edit_test_action_datum_path(test_action_datum) %></td>
        <td><%= link_to 'Destroy', test_action_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test action datum', new_test_action_datum_path %>
