<h1>Listing run_test_action_data</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Run object identifier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @run_test_action_data.each do |run_test_action_datum| %>
      <tr>
        <td><%= run_test_action_datum.data %></td>
