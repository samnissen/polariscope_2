<h1>Listing data_elements</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
      <th>Environment</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_elements.each do |data_element| %>
      <tr>
        <td><%= data_element.key %></td>
        <td><%= data_element.value %></td>
        <td><%= data_element.environment %></td>
        <td><%= data_element.user %></td>
        <td><%= link_to 'Show', data_element %></td>
        <td><%= link_to 'Edit', edit_data_element_path(data_element) %></td>
        <td><%= link_to 'Destroy', data_element, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data element', new_data_element_path %>
