<%- model_class = RunObjectIdentifier -%>
<div class="page-header">
  <h1><%=t '.title', :default => RunObjectIdentifier %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:identifier) %></dt>
      <dd><pre class="prettyprint"><%= @run_object_identifier.identifier %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:id_type) %></dt>
      <dd><pre class="prettyprint"><%= @run_object_identifier.id_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:selector) %></dt>
      <dd><pre class="prettyprint"><%= @run_object_identifier.selector %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              run_object_identifers_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_run_object_identifer_path(@run_object_identifier), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              run_object_identifer_path(@run_object_identifier),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
